{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": ["index.js", "modulo.js"],
    "includePattern": ".+\\.js(doc|x)?$"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "template": "node_modules/clean-jsdoc-theme",
    "tutorials": "./tutorials",
    "readme": "./tutorials/instruccion.md"
  }
}
